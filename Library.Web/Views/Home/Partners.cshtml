@inject IViewLocalizer Localizer
@{
    ViewData["Title"] = Localizer["Partners"];
    ViewData["Description"] = Localizer["List of Regional Library \'Mihalaki Georgiev\' partners."];
}
<div class="row text-center">
    <h2>@ViewData["Title"]</h2>
</div>
<div class="row">
    <div class="col-sm-2"></div>
    <div class="col-sm-3">
        <a class="box effect-1" href="http://vidin.bg">
            <img src="/images/SiteImages/VidinLogo.png" alt="@Localizer["Vidin Municipality"]" />
            <p>@Localizer["Vidin Municipality"]</p>
        </a>
    </div>
    <div class="col-sm-3">
        <a class="box effect-1" href="http://www.vidin.government.bg/094/60-16-16/index.html">
            <img src="/images/SiteImages/OAVidinLogo.jpg" alt="@Localizer["Regional Administration Vidin"]" />
            <p class="pt-25">@Localizer["Regional Administration Vidin"]</p>
        </a>
    </div>
    <div class="col-sm-3">
        <a class="box effect-1" href="http://www.nap.bg/page?id=24">
            <img src="/images/SiteImages/nap-logo.png" alt="@Localizer["TD of NRA Vidin"]" />
            <p>@Localizer["TD of NRA Vidin"]</p>
        </a>
    </div>
    <div class="col-sm-2"></div>
    <div class="col-sm-3">
        <a class="box effect-1" href="https://www.az.government.bg/contacts/offices/">
            <img src="/images/SiteImages/DBTLogo.png" alt="@Localizer["SBT Vidin"]" />
            <p>@Localizer["SBT Vidin"]</p>
        </a>
    </div>
    <div class="col-sm-3">
        <a class="box onvVidin effect-1" href="http://cmamyc.com/pic-vidin/msnv">
            <img src="/images/SiteImages/OSNVLogo.png" alt="@Localizer["Youth Municipal Council on Narcotic Substances"]" />
            <p class="p-t-20">@Localizer["Youth Municipal Council on Narcotic Substances"]</p>
        </a>
    </div>
    <div class="col-sm-3">
        <a class="box MPVidin effect-1" href="https://www.facebook.com/mladejki.parlament.vidin">
            <img src="/images/SiteImages/MPVidinLogo.jpg" alt="@Localizer["Kids Youth Parliament - Vidin"]" />
            <p class="pt-15">@Localizer["Kids Youth Parliament - Vidin"]</p>
        </a>
    </div>
    <div class="col-sm-2"></div>
    <div class="col-sm-3">
        <a class="box onvVidin effect-1" href="http://www.lib.bg/">
            <img src="/images/SiteImages/logobbia.jpg" alt="@Localizer["Bulgarian Library Information Association"]" />
            <p class="pt-15 font-size-94">@Localizer["Bulgarian Library Information Association"]</p>
        </a>
    </div>
    <div class="col-sm-3">
        <a class="box onvVidin effect-1" href="http://www.glbulgaria.bg/">
            <img src="/images/SiteImages/GB-logo.png" alt="@Localizer["Global Libraries Foundation Bulgaria"]" />
            <p class="pt-15">@Localizer["Global Libraries Foundation Bulgaria"]</p>
        </a>
    </div>
    <div class="col-sm-3">
        <a class="box MPVidin effect-1" href="http://mc.government.bg/">
            <img src="/images/SiteImages/MK-logo.jpg" alt="@Localizer["Ministry of Culture"]" />
            <p class="pt-35">@Localizer["Ministry of Culture"]</p>
        </a>
    </div>
    <div class="col-sm-2"></div>
    <div class="col-sm-3">
        <a class="box MPVidin effect-1" href="http://bnr.bg/vidin">
            <img src="/images/SiteImages/bnr-vidin.jpg" alt="@Localizer["BNR - Vidin"]" />
            <p class="pt-35">@Localizer["BNR - Vidin"]</p>
        </a>
    </div>

</div>


