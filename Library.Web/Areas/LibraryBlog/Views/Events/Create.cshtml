@model EventFormModel
@inject IViewLocalizer Localizer
@{
    ViewData["Title"] = Localizer["Create Event"];
}

<h1 class="text-center mr-t-b-25">@ViewData["Title"]</h1>
<div class="row">
    <div class="col-md-3"></div>
    <div class="col-md-6">
        @Html.Partial("_EventForm", Model)
    </div>
</div>

@section Scripts
    {
    @await Html.PartialAsync("_ValidationScriptsPartial")
}

